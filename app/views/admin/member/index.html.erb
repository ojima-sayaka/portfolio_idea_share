<div class="container">
  <h3>会員一覧</h3><br>
  <table class="table table-hover">
    <tr class="active">
      <th>登録日時</th>
      <th>会員画像</th>
      <th>会員名前</th>
      <th></th>
    </tr>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.created_at %></td>
        <td>
          <%= link_to admin_member_path(member) do %>
            <%= image_tag member.get_profile_image(50,50) %>
          <% end %>
        </td>
        <td><%= member.name %></td>
        <td>
          <!--投稿を削除-->
          <% if idea.member == current_member %>
            <%= link_to "Destroy", members_idea_path(idea), method: :delete, class: "btn-danger btn btn-sm "%>
          <% end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>