<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% members.each do |member| %>
      <tr>
        <td><%= image_tag member.get_profile_image(50,50) %></td>
        <td><%= member.name %></td>
        <td><%= link_to 'Show', member, class: "member_#{member.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>